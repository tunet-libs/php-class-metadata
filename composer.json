{
    "name": "tunet-libs/php-class-metadata",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aliaksandr Kulba",
            "email": "tunet000@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TunetLibs\\PhpClassMetadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TunetLibs\\PhpClassMetadata\\Tests\\": "src/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "phpstan/phpstan": "1.10.*",
        "squizlabs/php_codesniffer": "dev-master",
        "slevomat/coding-standard": "^8.12",
        "escapestudios/symfony2-coding-standard": "^3.13"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
